<header class="main-header main-header-style4">

    <!--Start header top Style3 -->
    <div class="header-top-style3">
        <div class="container">
            <div class="outer-box clearfix">
                <div class="header-top-left-style3 float-left">
                    {% if app.user is null %}
                        <div class="login-register-box">
                            <div class="icon-holder">
                                <span class="flaticon-user"></span>
                            </div>
                            <ul>
                                <li><a href="{{ path('app_register') }}">Créer un compte</a></li>
                                <li><a href="{{ path('app_login') }}">Se connecter</a></li>
                            </ul>
                        </div>
                    {% else %}
                        <div class="login-register-box">
                            <div class="icon-holder">
                                <span class="flaticon-user"></span>
                            </div>
                            <ul>
                                <li><a href="{{ path('app_front_profile_index') }}">Profil</a></li>
                                {% if is_granted('ROLE_ADMIN') %}
                                    <li><a href="{{ path('app_back_users_list') }}">Backoffice</a></li>{% endif %}
                                <li><a href="{{ path('app_logout') }}">Se déconnecter</a></li>
                            </ul>
                        </div>
                    {% endif %}
                </div>

                <!--Top Right-->
                <div class="header-top-right-style3 float-right">
                    <div class="header-social-links">
                        <ul class="sociallinks-style-two">
                            <li>
                                <a href="https://www.facebook.com/profile.php?id=100090616883418"><i
                                            class="fa fa-facebook" aria-hidden="true"></i></a>
                            </li>
                            <li>
                                <a href="mailto:contat.musge@gmail.com"><i class="fa fa-envelope"
                                                                           aria-hidden="true"></i></a>
                            </li>
                        </ul>
                    </div>
                </div>

            </div>
        </div>
    </div>
    <!--End header top Style3-->

    <!--Start Header upper Style4-->
    <div class="header-upper-style4">
        <div class="container clearfix">
            <div class="outer-box clearfix">
                <div class="header-upper-left-style4 clearfix float-left">
                    <div class="logo">
                        <a href={{ path('app_front_home') }}><img alt="Awesome Logo"
                                                                  src="{{ asset('images/logo_wh.png') }}"
                                                                  width="50"></a>
                    </div>
                </div>
                <div class="header-upper-right-style4 float-right clearfix">
                    <div class="nav-outer clearfix">
                        <!-- Main Menu -->
                        <nav class="main-menu style4 navbar-expand-lg">
                            <div class="navbar-header">
                                <!-- Toggle Button -->
                                <button type="button" class="navbar-toggle" data-toggle="collapse"
                                        data-target="#navbarSupportedContent"
                                        aria-controls="navbarSupportedContent" aria-expanded="false"
                                        aria-label="Toggle navigation">
                                    <span class="icon-bar"></span>
                                    <span class="icon-bar"></span>
                                    <span class="icon-bar"></span>
                                </button>
                            </div>

                            <div class="navbar-collapse collapse clearfix" id="navbarSupportedContent">
                                <ul class="navigation clearfix">
                                    <li><a href={{ path('app_front_home') }}>Accueil</a></li>
                                    <li><a href={{ path('app_front_about') }}>A propos</a></li>
                                    {% if app.user is not null %}
                                        {% if  app.user.abonnement is not null and ( app.user.abonnement.idOffre.type == "Bronze" or app.user.abonnement.idOffre.type == "Silver" or app.user.abonnement.idOffre.type == "Gold" ) %}
                                            <li><a href={{ path('app_front_categorie_index') }}>Galerie</a></li>
{#                                            <li><a href={{ path('app_front_avis_app_avis_index') }}>Avis</a></li>#}
                                        {% endif %}
                                        {% if app.user.abonnement is not null and ( app.user.abonnement.idOffre.type == "Silver" or app.user.abonnement.idOffre.type == "Gold" ) %}
                                            <li><a href={{ path('app_front_evenements_index') }}>Evenements</a></li>
                                        {% endif %}
                                        {% if app.user.abonnement is not null and ( app.user.abonnement.idOffre.type == "Gold" ) %}
                                            <li><a href={{ path('app_front_atelier') }}>Ateliers</a></li>
                                        {% endif %}
                                    {% endif %}
                                    <li><a href={{ path('app_front_offre_list') }}>Nos offres</a></li>
                                </ul>
                            </div>
                        </nav>
                        <!-- Main Menu End-->
                        <div class="menu-right-content-style4">
                            {#                                <div class="outer-search-box-style1"> #}
                            {#                                    <div class="seach-toggle"><i class="fa fa-search"></i></div> #}
                            {#                                    <ul class="search-box"> #}
                            {#                                        <li> #}
                            {#                                            <form method="post" action="http://mehedi.asiandevelopers.com/demo/museum/index.html"> #}
                            {#                                                <div class="form-group"> #}
                            {#                                                    <input type="search" name="search" placeholder="Search Here" required> #}
                            {#                                                    <button type="submit"><i class="fa fa-search"></i></button> #}
                            {#                                                </div> #}
                            {#                                            </form> #}
                            {#                                        </li> #}
                            {#                                    </ul> #}
                            {#                                </div> #}
                            {#                            <div class="buy-ticket-button"> #}
                            {#                                <a href={{ path('app_front_offre_list') }}>Nos offres</a> #}
                            {#                            </div> #}
                        </div>

                    </div>
                </div>
            </div>

        </div>
    </div>
    <!--End Header Upper Style4-->

    <!--Sticky Header-->
    <div class="sticky-header">
        <div class="container">
            <div class="clearfix">
                <!--Logo-->
                <div class="logo float-left">
                    <a href="{{ path('app_front_home') }}" class="img-responsive"><img
                                src="{{ asset('images/logo_wh.png') }}" alt="logo"></a>
                </div>
                <!--Right Col-->
                <div class="right-col float-right">
                    <!-- Main Menu -->
                    <nav class="main-menu navbar-expand-lg">
                        <div class="navbar-collapse collapse clearfix">
                            <ul class="navigation clearfix">
                                <li><a href={{ path('app_front_home') }}>Accueil</a></li>
                                <li><a href={{ path('app_front_about') }}>A propos</a></li>
                                {% if app.user is not null %}
                                    {% if  app.user.abonnement is not null and ( app.user.abonnement.idOffre.type == "Bronze" or app.user.abonnement.idOffre.type == "Silver" or app.user.abonnement.idOffre.type == "Gold" ) %}
                                        <li><a href={{ path('app_front_categorie_index') }}>Galerie</a></li>
{#                                        <li><a href={{ path('app_front_avis_app_avis_index') }}>Avis</a></li>#}
                                    {% endif %}
                                    {% if app.user.abonnement is not null and ( app.user.abonnement.idOffre.type == "Silver" or app.user.abonnement.idOffre.type == "Gold" ) %}
                                        <li><a href={{ path('app_front_evenements_index') }}>Evenements</a></li>
                                    {% endif %}
                                    {% if app.user.abonnement is not null and ( app.user.abonnement.idOffre.type == "Gold" ) %}
                                        <li><a href={{ path('app_front_atelier') }}>Ateliers</a></li>
                                    {% endif %}
                                {% endif %}
                                <li><a href={{ path('app_front_offre_list') }}>Nos offres</a></li>
                            </ul>
                        </div>
                    </nav><!-- Main Menu End-->
                </div>
            </div>
        </div>
    </div>
    <!--End Sticky Header-->
</header>