{% extends 'basefront.html.twig' %}
{% import "front-breadcrumb.html.twig" as breadcrumb %}

{% block title %}Offres{% endblock %}
{% block body %}
    <div class="boxed_wrapper">

        <div class="preloader"></div>

        <!--Start main header Style4-->
        {% include 'front-header.html.twig' %}
        <!--End main header Style4-->

        {{ breadcrumb.input('images/rami/hny_header_rami.jpg', 'Succés', 'Offres') }}

        <section class="contact-us-area">
            <div class="container">
                <div class="row">
                    <div class="col-xl-12">
                        <div class="left-content">
                            <p>Abonnement effectué avec succés!</p>
                        </div>
                        <div class="right-content">
                            <ul>
                                <li>
                                    <h3>Appelez-nous:-</h3>
                                    <p><a href="tel:16234569400">1 (623) 456-9400</a></p>
                                </li>
                                <li>
                                    <h3>Nos services:-</h3>
